{"components/psdc-common/wishlist-model":"<div class=\"wishlist wishlist-popup modal-body\">\n  <form class=\"form form-wishlist form-action\" method=\"post\" id=\"{{#if schema}}ProductWishlist{{else}}QuickViewProductWishlist{{/if}}\" action=\"{{product.add_to_wishlist_url}}\" data-wishlist-add>\n      <div class=\"form-action\">\n\n          <input type=\"hidden\" name=\"variation_id\" value=\"\">\n          {{# if customer.wishlists.length '>=' 0 }}\n           <div id=\"drop1\" data-dropdown-content>\n               <div style=\"padding: 5px;\" class=\"wishlist_dropdownbox\">\n                  {{# if customer.wishlists.length '>' 0 }}\n                   {{#each customer.wishlists}}\n                     <input class=\"wishlistid button button--small button--primary\" formaction=\"/wishlist.php?action=add&wishlistid={{id}}&product_id={{../product.id}}\" type=\"submit\" value=\"{{name}}\">\n                   {{/each}}\n                   {{else}}\n                   <p>Please create a new Wishlist</p>\n                   {{/if}}\n                   <div class=\"wishlist-popup-btn\">\n                     {{#if customer}}\n                       <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{../product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n                     {{/if}}\n\n                   </div>\n               </div>\n           </div>\n          {{/if}}\n      </div>\n  </form>\n</div>\n"}
