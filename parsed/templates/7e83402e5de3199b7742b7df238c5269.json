{"components/psdc-common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n  <ul class=\"navUser-section sf-menu\">\n      <li class=\"navUser-item currency-heading\">\n        <span>{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n      </li>\n      <li class=\"navUser-item navUser-item-currencySelector\">\n          <a class=\"navUser-action navUser-action--currencySelector\" href=\"#\" aria-controls=\"currencySelection\" aria-expanded=\"false\">\n            <span class=\"flag\">{{{lang 'common.currency-flag' flag=currency_selector.active_currency_flag}}}</span><span class=\"code\">{{lang 'common.currency-code' code=currency_selector.active_currency_code}}</span>\n            <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n          </a>\n          <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            {{#if is_active \"!=\" true}}\n              <li>\n                  <a href=\"{{{switch_url}}}\">\n                    <span class=\"flag\">{{{flag}}}</span><span class=\"code\">{{name}}</span>\n                  </a>\n              </li>\n            {{/if}}\n          {{/each}}\n          </ul>\n      </li>\n  </ul>\n{{/if}}\n","components/psdc-common/header":"<header class=\"header pepe\" sticky=\"{{theme_settings.sticky-header}}\" role=\"banner\">\r\n  <div class=\"header-main\">\r\n  <div class=\"container\">\r\n    <div class=\"header-inner\">\r\n      <div class=\"header-left\">\r\n        <div class=\"desktop_mobile_menu\">\r\n          <a class=\"control-otherlinks\" href=\"javascript:void(0);\" onclick=\"\">\r\n            <span class=\"otherlinks-line-1\"></span>\r\n            <span class=\"otherlinks-line-2\"></span>\r\n            <span class=\"otherlinks-line-3\"></span>\r\n            <span class=\"otherlinks-line-4\"></span>\r\n          </a>\r\n        </div>\r\n        <div class=\"logo-block\">\r\n          {{#if page_type '===' 'default'}}\r\n              <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                  {{> components/psdc-common/store-logo}}\r\n              </h1>\r\n          {{else}}\r\n              <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                  {{> components/psdc-common/store-logo}}\r\n              </div>\r\n          {{/if}}\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"header-center\">\r\n        <div class=\"navigation-menu\" data-menu>\r\n            {{> components/psdc-common/navigation-menu}}\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"header-right\">\r\n          {{> components/psdc-common/navigation}}\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"header-currancy\">\r\n    {{#if currency_selector.currencies.length '>' 1}}\r\n      <div class=\"navUser-item currency-item\">\r\n        <div class=\"currency\">\r\n          {{> components/psdc-common/currency-selector}}\r\n        </div>\r\n      </div>\r\n    {{/if}}\r\n  </div>\r\n  {{{region name=\"header_bottom\"}}}\r\n</div>\r\n  <div class=\"mobile-search\">\r\n    <div class=\"container\">\r\n      {{> components/psdc-common/quick-search}}\r\n    </div>\r\n  </div>\r\n</header>\r\n","components/psdc-common/header-account-menu":"<li class=\"navUser-item navUser-item--account\">\n    <a class=\"navUser-action needsclick\" href=\"{{urls.account.index}}\">\n      <svg><use xlink:href=\"#icon-user\"></use></svg>\n      <!-- <span>{{lang 'common.account'}}</span> -->\n    </a>\n    <ul class=\"navUser-section-sub section-sub-right\">\n      {{#if customer}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.auth.logout}}\">\n              <svg><use xlink:href=\"#icon-padlock\"></use></svg><span>{{lang 'common.logout'}}</span>\n            </a>\n        </li>\n      {{else}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.login}}\">\n            <svg><use xlink:href=\"#icon-signout\"></use></svg><span>{{lang 'common.login'}}</span>\n          </a>\n        </li>\n        {{#if settings.account_creation_enabled}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.create_account}}\">\n            <svg><use xlink:href=\"#icon-register\"></use></svg><span>{{lang 'common.sign_up'}}</span>\n          </a>\n        </li>\n        {{/if}}\n      {{/if}}\n      {{#if settings.gift_certificates_enabled}}\n          <li class=\"navUser-item\">\n              <a class=\"navUser-action needsclick\" href=\"{{urls.gift_certificate.purchase}}\">\n              <svg><use xlink:href=\"#icon-gift-certificate\"></use></svg><span>{{lang 'common.gift_cert'}}</span>\n              </a>\n          </li>\n      {{/if}}\n      {{#if settings.show_wishlist}}\n        <li class=\"navUser-item navUser-item-wishlist\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.account.wishlists.all}}\">\n              <svg><use xlink:href=\"#icon-wishlist\"></use></svg><span>{{lang 'account.nav.wishlist'}}</span>\n            </a>\n        </li>\n      {{/if}}\n\n      {{#if customer.store_credit.value '>' 0}}\n          <li class=\"navUser-item navUser-item-storeCredit\">\n              <a class=\"navUser-action navUser-action--storeCredit needsclick\" href=\"{{urls.cart}}\">\n                 <span>{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n              </a>\n          </li>\n      {{/if}}\n\n    </ul>\n</li>\n","components/psdc-common/navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt account-section sf-menu\">\n      {{> components/psdc-common/header-account-menu}}\n\n    {{#if theme_settings.EnableProductCompare}}\n      <li class=\"navUser-item navUser-item-compare\">\n          <a class=\"navUser-action navUser-item--compare needsclick\" title=\"Compare\" href=\"{{urls.compare}}\" data-compare-nav>\n            <svg><use xlink:href=\"#icon-compare\"></use></svg>\n            <span class=\"countPill countPill--positive\"></span>\n          </a>\n      </li>\n    {{/if}}\n\n      <li class=\"navUser-item navUser-search\">\n          <a class=\"navUser-action navUser-action--quickSearch\" href=\"javascript:void(0);\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><svg class=\"search-icon\"><use xlink:href=\"#icon-search\"></use></svg> <svg class=\"search-close\"><use xlink:href=\"#icon-close\"></use></svg></a>\n      </li>\n\n\n      <li class=\"navUser-item navUser-item--cart\">\n          <a  class=\"navUser-action\"\n              data-cart-preview\n              data-dropdown=\"cart-preview-dropdown\"\n              data-options=\"align:right\"\n              href=\"{{urls.cart}}\">\n              <span class=\"navUser-item-cartLabel\">\n                <svg><use xlink:href=\"#icon-cart\"></use></svg>\n              </span> <span class=\"countPill cart-quantity\"></span>\n          </a>\n          <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n      </li>\n    </ul>\n\n</nav>\n\n<div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n    {{> components/psdc-common/quick-search headerSearch='headerSearch'}}\n</div>\n\n<div id=\"headerSidebar\" class=\"header-side-bar\">\n<div class=\"header-side-bar-inn\">\n  <div class=\"close\">\n    <a class=\"close-icon\" href=\"javascript:void(0);\"><svg><use xlink:href=\"#icon-close\"></use></svg></a>\n  </div>\n  <div class=\"category headerSidebar-block\">\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.categories'}}</h5>\n    <ul class=\"navList navList-treeview\">\n        {{#each categories}}\n            <li class=\"navPages-item\">\n              {{> components/psdc-common/navigation-list}}\n            </li>\n      {{/each}}\n    </ul>\n  </div>\n  <div class=\"webpages headerSidebar-block\">\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.navigate'}}</h5>\n    <ul class=\"navList navList-treeview\">\n      {{#each pages}}\n           <li class=\"navPages-item navPages-item-page\">\n             {{> components/psdc-common/navigation-list}}\n           </li>\n       {{/each}}\n    </ul>\n  </div>\n  <div class=\"brand headerSidebar-block\">\n    <h5 class=\"sidebarBlock-heading\">{{lang 'footer.brands'}}</h5>\n    <ul class=\"navList navList-treeview\">\n      {{#each shop_by_brand}}\n          {{#if @index '<' '5' }}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{url}}\">\n                  <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></svg></span>\n                  {{name}}\n                </a>\n            </li>\n          {{/if}}\n      {{/each}}\n    </ul>\n  </div>\n  <div class=\"brand headerSidebar-block mobile-currancy\">\n    <h5 class=\"sidebarBlock-heading\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <span class=\"flag\">{{{lang 'common.currency-flag' flag=currency_selector.active_currency_flag}}}</span><span class=\"code\">{{lang 'common.currency-code' code=currency_selector.active_currency_code}}</span></h5>\n\n  </div>\n</div>\n\n</div>\n","components/psdc-common/navigation-list":"<!-- SECOND LEVEL STARTS -->\n{{#if children}}\n\n<a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n  <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n    {{name}}\n    <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n</a>\n<ul class=\"navlist-sub nav-second-level\">\n    {{#each children}}\n    <li>\n        <!-- THIRD LEVEL STARTS -->\n        {{#if children}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n            {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n        </a>\n        <ul class=\"navlist-sub nav-third-level\">\n            {{#each children}}\n            <li>\n              <!-- FOURTH LEVEL STARTS -->\n              {{#if children}}\n              <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n                <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n                  {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n              </a>\n              <ul class=\"navlist-sub nav-third-level\">\n                  {{#each children}}\n                  <li>\n                      <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}} </a>\n                  </li>\n                  {{/each}}\n              </ul>\n              {{else}}\n                <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\n              {{/if}}\n              <!-- FOURTH LEVEL STARTS -->\n\n            </li>\n            {{/each}}\n        </ul>\n        {{else}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>{{name}}</a>\n        {{/if}}\n        <!-- THIRD LEVEL STARTS -->\n    </li>\n    {{/each}}\n</ul>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">\n  <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>{{name}}</a>\n{{/if}}\n<!-- SECOND LEVEL ENDS -->\n","components/psdc-common/navigation-menu":"<nav class=\"navPages\">\n  <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} sf-menu\" data-speed=\"0.8\">\n\n    {{#if theme_settings.headernavigation '==' \"navcategory\"}}\n        <li class=\"{{#if @last}}last-menu-item{{/if}}\">\n            {{#each categories}}\n                  <li class=\"navPages-item\">\n                    {{> components/psdc-common/navigation-list}}\n                  </li>\n            {{/each}}\n        </li>\n    {{/if}}\n\n\n    {{#if theme_settings.headernavigation '==' \"navwebpage\"}}\n      {{#each pages}}\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\n             {{> components/psdc-common/navigation-list}}\n           </li>\n       {{/each}}\n    {{/if}}\n\n{{#if theme_settings.headernavigation '==' \"navboth\"}}\n      <li class=\"navPages-item\">\n        <a class=\"navPages-action needsclick\" href=\"/categories\">{{theme_settings.category-menu-name}} <svg><use xlink:href=\"#icon-chevron-down\"></use></svg></a>\n        <ul>\n          {{#each categories}}\n            <li class=\"navPages-item {{#and @last (if (length pages) '==' '0')}}last-menu-item{{/and}} nav-arrow-roted\">\n              {{> components/psdc-common/navigation-list}}\n            </li>\n          {{/each}}\n        </ul>\n      </li>\n      {{#each pages}}\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\n             {{> components/psdc-common/navigation-list}}\n           </li>\n       {{/each}}\n  {{/if}}\n  </ul>\n</nav>\n","components/psdc-common/quick-search":"<form class=\"form\" action=\"{{urls.search}}\"  id=\"{{#if headerSearch}}headerQuickSearchForm{{else}}mobileQuickSearchForm{{/if}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            <span class=\"search-icon-main\" onclick=\"{{#if headerSearch}}document.getElementById('headerQuickSearchForm').submit();{{else}}document.getElementById('mobileQuickSearchForm').submit();{{/if}}\"><svg class=\"search-icon\"><use xlink:href=\"#icon-search\"></use></svg></span>\n        </div>\n    </fieldset>\n</form>\n<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n","components/psdc-common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container-custom\">\n                <img class=\"header-logo-image-custom\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
