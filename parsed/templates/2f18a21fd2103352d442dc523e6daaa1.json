{"components/psdc-common/header-account-menu":"<li class=\"navUser-item navUser-item--account\">\n    <a class=\"navUser-action needsclick\" href=\"{{urls.account.index}}\">\n      <svg><use xlink:href=\"#icon-user\"></use></svg>\n      <!-- <span>{{lang 'common.account'}}</span> -->\n    </a>\n    <ul class=\"navUser-section-sub section-sub-right\">\n      {{#if customer}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.auth.logout}}\">\n              <svg><use xlink:href=\"#icon-padlock\"></use></svg><span>{{lang 'common.logout'}}</span>\n            </a>\n        </li>\n      {{else}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.login}}\">\n            <svg><use xlink:href=\"#icon-signout\"></use></svg><span>{{lang 'common.login'}}</span>\n          </a>\n        </li>\n        {{#if settings.account_creation_enabled}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.create_account}}\">\n            <svg><use xlink:href=\"#icon-register\"></use></svg><span>{{lang 'common.sign_up'}}</span>\n          </a>\n        </li>\n        {{/if}}\n      {{/if}}\n      {{#if settings.gift_certificates_enabled}}\n          <li class=\"navUser-item\">\n              <a class=\"navUser-action needsclick\" href=\"{{urls.gift_certificate.purchase}}\">\n              <svg><use xlink:href=\"#icon-gift-certificate\"></use></svg><span>{{lang 'common.gift_cert'}}</span>\n              </a>\n          </li>\n      {{/if}}\n      {{#if settings.show_wishlist}}\n        <li class=\"navUser-item navUser-item-wishlist\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.account.wishlists.all}}\">\n              <svg><use xlink:href=\"#icon-wishlist\"></use></svg><span>{{lang 'account.nav.wishlist'}}</span>\n            </a>\n        </li>\n      {{/if}}\n\n      {{#if customer.store_credit.value '>' 0}}\n          <li class=\"navUser-item navUser-item-storeCredit\">\n              <a class=\"navUser-action navUser-action--storeCredit needsclick\" href=\"{{urls.cart}}\">\n                 <span>{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n              </a>\n          </li>\n      {{/if}}\n\n    </ul>\n</li>\n"}
