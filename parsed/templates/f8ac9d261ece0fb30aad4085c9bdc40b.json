{"components/psdc-common/navigation-list":"<!-- SECOND LEVEL STARTS -->\n{{#if children}}\n\n<a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n  <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n    {{name}}\n    <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n</a>\n<ul class=\"navlist-sub nav-second-level\">\n    {{#each children}}\n    <li>\n        <!-- THIRD LEVEL STARTS -->\n        {{#if children}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n            {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n        </a>\n        <ul class=\"navlist-sub nav-third-level\">\n            {{#each children}}\n            <li>\n              <!-- FOURTH LEVEL STARTS -->\n              {{#if children}}\n              <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n                <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>\n                  {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n              </a>\n              <ul class=\"navlist-sub nav-third-level\">\n                  {{#each children}}\n                  <li>\n                      <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}} </a>\n                  </li>\n                  {{/each}}\n              </ul>\n              {{else}}\n                <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\n              {{/if}}\n              <!-- FOURTH LEVEL STARTS -->\n\n            </li>\n            {{/each}}\n        </ul>\n        {{else}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>{{name}}</a>\n        {{/if}}\n        <!-- THIRD LEVEL STARTS -->\n    </li>\n    {{/each}}\n</ul>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">\n  <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></span>{{name}}</a>\n{{/if}}\n<!-- SECOND LEVEL ENDS -->\n","components/psdc-common/navigation-menu":"<nav class=\"navPages\">\n  <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}} sf-menu\" data-speed=\"0.8\">\n\n    {{#if theme_settings.headernavigation '==' \"navcategory\"}}\n        <li class=\"{{#if @last}}last-menu-item{{/if}}\">\n            {{#each categories}}\n                  <li class=\"navPages-item\">\n                    {{> components/psdc-common/navigation-list}}\n                  </li>\n            {{/each}}\n        </li>\n    {{/if}}\n\n\n    {{#if theme_settings.headernavigation '==' \"navwebpage\"}}\n      {{#each pages}}\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\n             {{> components/psdc-common/navigation-list}}\n           </li>\n       {{/each}}\n    {{/if}}\n\n{{#if theme_settings.headernavigation '==' \"navboth\"}}\n      <li class=\"navPages-item\">\n        <a class=\"navPages-action needsclick\" href=\"/categories\">{{theme_settings.category-menu-name}} <svg><use xlink:href=\"#icon-chevron-down\"></use></svg></a>\n        <ul>\n          {{#each categories}}\n            <li class=\"navPages-item {{#and @last (if (length pages) '==' '0')}}last-menu-item{{/and}} nav-arrow-roted\">\n              {{> components/psdc-common/navigation-list}}\n            </li>\n          {{/each}}\n        </ul>\n      </li>\n      {{#each pages}}\n           <li class=\"navPages-item navPages-item-page {{#if @last}}last-menu-item{{/if}}\">\n             {{> components/psdc-common/navigation-list}}\n           </li>\n       {{/each}}\n  {{/if}}\n  </ul>\n</nav>\n"}
